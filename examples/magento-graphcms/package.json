{
  "name": "@graphcommerce/magento-graphcms",
  "homepage": "https://www.graphcommerce.org/",
  "repository": "github:graphcommerce-org/graphcommerce",
  "version": "6.0.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "16.x.x||18.x.x"
  },
  "scripts": {
    "dev": "concurrently -k -n codegen,next 'graphql-codegen -w' 'next dev'",
    "codegen": "yarn graphcommerce codegen-config && mesh build && graphql-codegen",
    "build": "mesh build && graphql-codegen && next build && next-sitemap",
    "start": "next start",
    "tsc:lint": "tsc --noEmit -p .",
    "lingui": "cross-env NODE_ENV=development lingui extract --clean",
    "postinstall": "is-monorepo '[pkgrun] postinstall' '[pkgrun] patch-package'",
    "create-patch": "patch-package --exclude 'package.json$|gql.ts$|interceptor.tsx$'"
  },
  "dependencies": {
    "@graphcommerce/cli": "6.0.0",
    "@graphcommerce/demo-magento-graphcommerce": "6.0.0",
    "@graphcommerce/ecommerce-ui": "6.0.0",
    "@graphcommerce/framer-next-pages": "6.0.0",
    "@graphcommerce/framer-scroller": "6.0.0",
    "@graphcommerce/framer-utils": "6.0.0",
    "@graphcommerce/googleanalytics": "6.0.0",
    "@graphcommerce/googlerecaptcha": "6.0.0",
    "@graphcommerce/googletagmanager": "6.0.0",
    "@graphcommerce/graphcms-ui": "6.0.0",
    "@graphcommerce/graphql": "6.0.0",
    "@graphcommerce/graphql-mesh": "6.0.0",
    "@graphcommerce/image": "6.0.0",
    "@graphcommerce/lingui-next": "6.0.0",
    "@graphcommerce/magento-cart": "6.0.0",
    "@graphcommerce/magento-cart-billing-address": "6.0.0",
    "@graphcommerce/magento-cart-checkout": "6.0.0",
    "@graphcommerce/magento-cart-coupon": "6.0.0",
    "@graphcommerce/magento-cart-email": "6.0.0",
    "@graphcommerce/magento-cart-items": "6.0.0",
    "@graphcommerce/magento-cart-payment-method": "6.0.0",
    "@graphcommerce/magento-cart-shipping-address": "6.0.0",
    "@graphcommerce/magento-cart-shipping-method": "6.0.0",
    "@graphcommerce/magento-category": "6.0.0",
    "@graphcommerce/magento-cms": "6.0.0",
    "@graphcommerce/magento-customer": "6.0.0",
    "@graphcommerce/magento-graphql": "6.0.0",
    "@graphcommerce/magento-newsletter": "6.0.0",
    "@graphcommerce/magento-payment-included": "6.0.0",
    "@graphcommerce/magento-product": "6.0.0",
    "@graphcommerce/magento-product-bundle": "6.0.0",
    "@graphcommerce/magento-product-configurable": "6.0.0",
    "@graphcommerce/magento-product-downloadable": "6.0.0",
    "@graphcommerce/magento-product-grouped": "6.0.0",
    "@graphcommerce/magento-product-simple": "6.0.0",
    "@graphcommerce/magento-product-virtual": "6.0.0",
    "@graphcommerce/magento-review": "6.0.0",
    "@graphcommerce/magento-search": "6.0.0",
    "@graphcommerce/magento-store": "6.0.0",
    "@graphcommerce/magento-wishlist": "6.0.0",
    "@graphcommerce/next-config": "6.0.0",
    "@graphcommerce/next-ui": "6.0.0",
    "@lingui/core": "3.17.1",
    "@lingui/react": "3.17.1",
    "@mui/lab": "5.0.0-alpha.112",
    "@mui/material": "5.11.0",
