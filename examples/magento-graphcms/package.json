    "graphql": "16.6.0",
    "next": "13.1.6",
    "next-pwa": "5.6.0",
    "next-sitemap": "3.1.42",
    "patch-package": "6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "0.31.2",
    "webpack": "5.75.0"
  },
  "devDependencies": {
    "@graphcommerce/eslint-config-pwa": "6.0.0",
    "@graphcommerce/prettier-config-pwa": "6.0.0",
    "@graphcommerce/typescript-config-pwa": "6.0.0",
    "@lingui/cli": "3.17.1",
    "eslint": "8.29.0",
    "prettier": "2.8.1",
    "type-fest": "^3.4.0",
    "typescript": "4.9.4"
  },
  "browserslist": [
    "> 1% in alt-EU",
    "not IE 11"
  ],
  "prettier": "@graphcommerce/prettier-config-pwa",
  "eslintConfig": {
    "extends": "@graphcommerce/eslint-config-pwa",
